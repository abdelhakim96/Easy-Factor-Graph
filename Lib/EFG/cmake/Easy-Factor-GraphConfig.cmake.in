get_filename_component(EFG_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(EFG_PROJECT_NAME Easy-Factor-Graph)
list(APPEND CMAKE_MODULE_PATH ${EFG_CMAKE_DIR})
include(CMakeFindDependencyMacro)

list(REMOVE_AT CMAKE_MODULE_PATH -1)

if(NOT TARGET EFG::${EFG_PROJECT_NAME})
  include("${EFG_CMAKE_DIR}/${EFG_PROJECT_NAME}Targets.cmake")
  set_property(TARGET EFG::${EFG_PROJECT_NAME} APPEND PROPERTY VERSION  ${${EFG_PROJECT_NAME}_VERSION})
endif()

set(${EFG_PROJECT_NAME}_LIBRARIES ${EFG_PROJECT_NAME})
